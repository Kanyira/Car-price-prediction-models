{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "eastus"
    },
    "environmentName": {
      "value": "car-price-env"
    },
    "backendAppName": {
      "value": "car-price-backend"
    },
    "frontendAppName": {
      "value": "car-price-frontend"
    },
    "containerRegistry": {
      "value": "<YOUR_REGISTRY>.azurecr.io"
    },
    "backendImage": {
      "value": "<YOUR_REGISTRY>.azurecr.io/car-price-backend:latest"
    },
    "frontendImage": {
      "value": "<YOUR_REGISTRY>.azurecr.io/car-price-frontend:latest"
    },
    "registryUsername": {
      "value": "<YOUR_REGISTRY_USERNAME>"
    },
    "registryPassword": {
      "value": "<YOUR_REGISTRY_PASSWORD>"
    }
  }
}
